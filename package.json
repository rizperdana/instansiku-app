        "vee-validate": "^2.2.15",
        "vue": "^2.6.14",
        "vue-chartist": "^2.3.1",
        "vue-i18n": "^8.24.1",
        "vue-router": "^3.5.2",
        "vue-rx": "^6.2.0",
        "vue-world-map": "^0.1.1",
        "vue2-editor": "^2.10.2",
        "vuedraggable": "^2.24.3",
        "vuejs-clipper": "^3.1.2",
        "vuetify": "^2.5.7",
        "vuex": "^3.6.2"
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/eslint-parser": "^7.14.7",
        "@vue/cli-plugin-babel": "~3.11.0",
